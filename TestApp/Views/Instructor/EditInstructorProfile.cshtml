@model TestApp.Models.Instructor

<h2>Edit Profile</h2>

<form asp-action="EditInstructorProfile" method="post" enctype="multipart/form-data">
    <input type="hidden" asp-for="InstructorID" />

    <div class="form-group">
        <label asp-for="instructor_name" class="form-label"></label>
        <input asp-for="instructor_name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="latest_qualification" class="form-label"></label>
        <input asp-for="latest_qualification" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="expertise_area" class="form-label"></label>
        <input asp-for="expertise_area" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="email" class="form-label"></label>
        <input asp-for="email" class="form-control" />
    </div>

    <button type="submit" class="btn btn-primary">Save Changes</button>
    <a asp-action="InstructorDashboard" class="btn btn-secondary">Cancel</a>
</form>
